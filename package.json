{
  "name": "supabase-rr-middleware",
  "version": "1.0.1",
  "description": "React Router middleware for Supabase authentication and session management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:typescript": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikePresman/supabase-rr-middleware.git"
  },
  "keywords": [
    "supabase",
    "auth",
    "middleware",
    "react-router-7",
    "react-router",
    "remix"
  ],
  "author": "Mike Presman <mikepresman@gmail.com>",
  "contributors": [
    {
      "name": "Mike Presman",
      "url": "https://github.com/MikePresman"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikePresman/supabase-rr-middleware/issues"
  },
  "homepage": "https://github.com/MikePresman/supabase-rr-middleware",
  "funding": [],
  "peerDependencies": {
    "@supabase/ssr": ">=0.52.0",
    "@supabase/supabase-js": ">=2.49.0",
    "react-router": "^7.3.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "react-router": "^7.4.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.29.0",
    "uvu": "^0.5.6",
    "vitest": "^3.1.1"
  }
}
